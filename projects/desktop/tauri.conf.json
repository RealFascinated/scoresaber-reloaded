  "productName": "ScoreSaber Reloaded",
  "version": "0.1.0",
  "identifier": "cc.fascinated.ssr",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "",
    "frontendDist": "https://ssr.fascinated.cc"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "ScoreSaber Reloaded",
