{
  "name": "scoresaber-reloaded",
  "version": "1.0.0",
  "workspaces": {
    "nohoist": [
      "**/projects/website/**",
      "**/projects/website"
    ],
    "packages": [
      "projects/*"
    ]
  },
  "scripts": {
    "dev:website": "bun --filter 'website' dev",
    "dev:backend": "bun --filter 'backend' dev",
    "dev:common": "bun --filter '@ssr/common' dev",
    "prettier": "prettier --write .",
    "count-lines": "find projects -type f -not -path '*/dist/*' -not -path '*/.next/*' -name '*.ts' -o -name '*.tsx' | xargs wc -l"
  },
  "author": "fascinated7",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "superjson": "^2.2.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.17"
  }
}
